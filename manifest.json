{
  // Required

  "manifest_version": 2,
  "name": "Speed Dial Bookmarks",
  "description": "This extension is a quick speed dialffor all your bookmarks. Get it to quicken your browser experience.",
  "version": "1.0",

  "browser_action": {
          "default_icon": {                    // optional
            "19": "19x19.png",// optional
            "38": "32x32.png"
          },
          "default_title": "SpeedD - Speed dial",      // optional; shown in tooltip
          "default_popup": "popup.html"        // optional
        },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "identity",
    "identity.email"
   
  ],
  "author" : "subodh shetty"
}
